{
  "resourceType": "Patient",
  "id": "{{ id | default: uuid }}",
  "identifier": [
    {
      "system": "{{ identifier_system | default: 'urn:oid:1.2.3.4.5' }}",
      "value": "{{ identifier_value }}"
    }
  ],
  "active": {{ active | default: true }},
  "name": [
    {
      "use": "official",
      "family": "{{ family_name }}",
      "given": {{ given_names | json }}
    }
  ]
  {% if gender %}
  ,"gender": "{% case gender | downcase %}{% when 'm' %}male{% when 'f' %}female{% else %}{{ gender }}{% endcase %}"
  {% endif %}
  {% if birth_date %}
  ,"birthDate": "{{ birth_date }}"
  {% endif %}
}